{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "npx rimraf dist",
    "build": "npx nest build",
    "format": "npx prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main",
    "start:dev": "npx nest start --watch",
    "start:debug": "npx nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "npx eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "npx jest",
    "test:watch": "npx jest --watch",
    "test:cov": "npx jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.3.1",
    "@nestjs/common": "^6.11.7",
    "@nestjs/core": "^6.11.7",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/mongoose": "^6.3.1",
    "@nestjs/passport": "^6.1.1",
    "@nestjs/platform-express": "^6.11.7",
    "@nestjs/swagger": "^4.2.8",
    "@nestjs/terminus": "^6.5.6",
    "bcrypt": "^3.0.8",
    "cache-manager": "^2.11.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.11.0",
    "mongoose": "^5.8.11",
    "mysql": "^2.18.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.14.2",
    "@nestjs/schematics": "^6.9.3",
    "@nestjs/testing": "^6.11.7",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.9.1",
    "@types/mongodb": "^3.3.16",
    "@types/mongoose": "^5.7.0",
    "@types/node": "^13.1.6",
    "@types/passport": "^1.0.2",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleDirectories": [
      "node_modules",
      "./"
    ],
    "rootDir": "./",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
